<%= javascript_include_tag "admin/application" %>

<%=raw title 'Новая страница' %>

<%= form_for(@page) do |f| %>
  <%#= f.error_messages %>

  <p>
    Название<br />
    <%= f.text_field :title %>
  </p>
  <p>
    Текст<br />
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.submit 'Создать' %>
  </p>
<% end %>

<%= link_to 'Назад', pages_path %>