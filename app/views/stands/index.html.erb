<p id="notice"><%= notice %></p>

<h1>Stands</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Artikul</th>
      <th>Price</th>
      <th>Pricefigure</th>
      <th>Size</th>
      <th>Format</th>
      <th>Quantity</th>
      <th>Additional</th>
      <th>Image file name</th>
      <th>Image content type</th>
      <th>Image file size</th>
      <th>Image updated at</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stands.each do |stand| %>
      <tr>
        <td><%= stand.name %></td>
        <td><%= stand.artikul %></td>
        <td><%= stand.price %></td>
        <td><%= stand.pricefigure %></td>
        <td><%= stand.size %></td>
        <td><%= stand.format %></td>
        <td><%= stand.quantity %></td>
        <td><%= stand.additional %></td>
        <td><%= stand.image_file_name %></td>
        <td><%= stand.image_content_type %></td>
        <td><%= stand.image_file_size %></td>
        <td><%= stand.image_updated_at %></td>
        <td><%= stand.category %></td>
        <td><%= link_to 'Show', stand %></td>
        <td><%= link_to 'Edit', edit_stand_path(stand) %></td>
        <td><%= link_to 'Destroy', stand, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stand', new_stand_path %>
